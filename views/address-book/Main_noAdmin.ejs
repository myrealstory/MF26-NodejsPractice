<%- include('../parts/html-head') %> 
<%- include('../parts/navbar') %>

<div class="container">
    <div class="row">
        <div class="col">
            <nav aria-label="Page navigation example">
                <ul class="pagination">
                  <li class="page-item">
                    <a class="page-link" href="#">Previous</a>
                    </li>

                    <% for(let i=page-5; i<=page+5; i++) if(i>0 
                    ){ 
                        const queryobj =  {page: i };
                        if(query.search){
                            queryobj.search = query.search;
                        }
                         
                        if(query.beginDate){
                            queryobj.beginDate = query.beginDate;
                        }
                        if(query.endDate){
                            queryobj.endDate = query.endDate;
                        }

                        const myLink = new URLSearchParams(queryobj).toString();
                        
                        %>
                    <li class="page-item <%= page===i ? 'active' : '' %>">
                        <a class="page-link" href="?<%= myLink %>"><%= i %></a>
                    </li>
                    <% } %>
                    <!-- <li class="page-item">
                        <a class="page-link" href="?page=1">1</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="?page=2">2</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="?page=3">3</a>
                    </li> -->
                  <li class="page-item"><a class="page-link" href="#">Next</a></li>
                </ul>
              </nav>
        </div>
    </div>


    <div class="row">
        <div class="col">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">sid</th>
                    <th scope="col">name</th>
                   
                  </tr>
                </thead>
                <tbody>
                    <% for(let i of rows){ %>
                  <tr>
                    <td><%= i.sid %></td>
                    <td><%= i.name %></td>
                   
                  </tr>
                 <% } %>
                </tbody>
              </table>
        </div>
    </div>
</div>

<%- include('../parts/script')%>
<script>
    const rows = <%- JSON.stringify(rows) %>;

</script>
<%- include('../parts/html-foot') %>